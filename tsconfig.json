{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "rootDir": "./",
        "outDir": "./build",
        "resolveJsonModule": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": false,
        "lib": [
            "es6"
        ]
    }
}